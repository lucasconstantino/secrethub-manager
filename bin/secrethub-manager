#!/usr/bin/env node

const { Command } = require("commander");
const { version } = require("../package.json");

const quit = (err) => {
  console.error(err);
  process.exit(1);
};

new Command()
  .version(version)
  .addCommand(require("../commands/init"))
  .addCommand(require("../commands/save"))
  .parseAsync(process.argv)
  .catch(quit);
